			<text:p text:style-name="Title">金萱字型安裝教學</text:p>
			<text:p text:style-name="P55">
				<draw:frame draw:style-name="fr2" draw:name="影像1" text:anchor-type="as-char" svg:width="5.002cm" svg:height="5.406cm" draw:z-index="0">
